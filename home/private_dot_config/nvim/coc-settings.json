    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
  ],
  "coc.preferences.diagnostic.virtualText": true,
  "coc.source.emoji.filetypes": ["markdown", "typescript"]
}
